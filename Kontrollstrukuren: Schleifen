//Java Einstieg Woche2 Kapitel4 Aufgabe4 openhpi
class Robot {
    EncryptedEMail encryptedEMail = new EncryptedEMail();
    public void decryptSender(){
        for (int i = 0; i < 4; i++ ){
            for (int j = 0; j < 28; j++){
                System.out.print(encryptedEMail.decryptChar(i, j));
            }
            System.out.println();
        }
    }

}

class Story {

    public static void main(String[] args) {
        Robot robin = new Robot();
        robin.decryptSender();
    }

}
