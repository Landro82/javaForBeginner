/*Java Einstieg Woche4 Kapitel1 Aufgabe1*/
public class Robot {

	private String name;
	private String id;
	private int age;

	Robot(String name, String id, int age) {
		this.name = name;
		this.id = id;
		this.age = age;
	}
	
	public String toString(){
	    return name + "_" + id + "_"+ age;
	  	  
	}
}

public class Story {

	public static void main(String[] args) {
		Robot ronja = new Robot("Ronja", "ak2jsd3125", 2);
		Robot robin = new Robot("Robin", "asd453ln", 5);
		System.out.println(ronja.toString());
		System.out.println(robin.toString()); 
	}
}
